{
    "name": "@dotbox/vscode",
    "displayName": "DotBox",
    "description": "DotBox language support",
    "version": "0.1.0",
    "titan": {
        "publish": false
    },
    "vsce": {
        "message": "chore(release): publish",
        "no-git-tag-version": true,
        "no-update-package-json": true
    },
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dotbox",
                "aliases": [
                    "DotBox",
                    "dotbox"
                ],
                "extensions": [
                    ".box"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dotbox",
                "scopeName": "source.box",
                "path": "./syntaxes/dotbox.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "vitest": "^0.10.0"
    },
    "homepage": "https://github.com/jakehamilton/dotbox",
    "repository": {
        "type": "git",
        "url": "https://github.com/jakehamilton/dotbox.git"
    },
    "bugs": {
        "url": "https://github.com/jakehamilton/dotbox/issues"
    }
}
